
EXITCODE=1

while [ $EXITCODE != 0 ] 
do

    rm output*.log

    RUST_LOG=wasm_mutate,analyzer,wasm_shrink=debug ./target/debug/analyzer admin admin -t canonical reduce -s  -f ../RQ1/all-binaries-metadata/all -o reduced
    EXITCODE=$?


    if [ $EXITCODE != 0 ]
    then
        echo "Restarting..."
    fi
done